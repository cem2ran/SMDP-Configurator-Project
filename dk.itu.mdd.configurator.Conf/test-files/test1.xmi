<?xml version="1.0" encoding="ASCII"?>
<modelMDD2:Model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:modelMDD2="http://www.example.org/modelMDD2"
    xsi:schemaLocation="http://www.example.org/modelMDD2 ../../asd/model/modelMDD2.ecore"
    name="CarConfigurator">
  <root name="Car">
    <subfeature
        xsi:type="modelMDD2:Mandatory"
        name="Engine">
      <groups
          xsi:type="modelMDD2:Xor"
          name="Horsepower">
        <grouped
            name="HP120">
          <attributes
              xsi:type="modelMDD2:Number"
              value="120"/>
        </grouped>
        <grouped
            name="HP150">
          <attributes
              xsi:type="modelMDD2:Number"
              value="150"/>
        </grouped>
      </groups>
      <groups
          xsi:type="modelMDD2:Xor"
          name="Fish">
        <grouped
            name="Cat"/>
        <grouped
            name="Dog"/>
      </groups>
    </subfeature>
    <subfeature
        xsi:type="modelMDD2:Optional"
        name="Rims">
      <groups
          xsi:type="modelMDD2:Or"
          name="Color">
        <grouped
            name="MatteBlack">
          <constrains
              xsi:type="modelMDD2:Binary"
              Operator="&amp;&amp;">
            <rightExp
                featureReference="//@root/@subfeature.0/@groups.0/@grouped.0"/>
            <leftExp
                featureReference="//@root/@subfeature.1/@groups.1/@grouped.0"/>
          </constrains>
        </grouped>
        <grouped
            name="Silver"/>
      </groups>
      <groups
          xsi:type="modelMDD2:Xor"
          name="Material">
        <grouped
            name="Aluminum"/>
        <grouped
            name="CarbonFiber"/>
      </groups>
    </subfeature>
  </root>
</modelMDD2:Model>
